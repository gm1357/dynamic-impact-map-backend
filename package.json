{
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "prisma:generate": "prisma generate --schema ./infra/db/schema.prisma",
    "prisma:migrate": "prisma migrate --schema ./infra/db/schema.prisma",
    "postinstall": "yarn prisma:generate"
  },
  "prisma": {
    "schema": "./infra/db/schema.prisma",
    "seed": "ts-node infra/db/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.6",
    "nodemon": "^3.1.7",
    "prisma": "^5.21.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
